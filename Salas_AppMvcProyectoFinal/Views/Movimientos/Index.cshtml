@model IEnumerable<Salas_AppMvc06.Models.Movimiento>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @*@Html.ActionLink("Create New", "Create")*@
    <button class="btn-create">Nuevo Movimiento</button>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CtaOrigen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CtaDestino)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CodTipoMovimiento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CtaOrigen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CtaDestino)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CodTipoMovimiento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id = item.Codigo }) |
            @Html.ActionLink("Detalles", "Details", new { id = item.Codigo }) |
            @Html.ActionLink("Eliminar", "Delete", new { id = item.Codigo })
        </td>
    </tr>
}

</table>
<div id="modal"></div>

<script src="../../Scripts/jquery-1.10.2.min.js" type="text/javascript"></script>
<script src="../../Scripts/jsEventos.js" type="text/javascript"></script>